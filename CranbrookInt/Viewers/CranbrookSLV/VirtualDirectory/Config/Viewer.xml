        <View Id="TimeAwareSliderConfigView" Region="ModalWindowRegion" Type="Geocortex.EssentialsSilverlightViewer.CoreModules.TimeAwareSlider.TimeAwareSliderConfigView" Visible="false" />
      </Views>
      <Configuration>
          {TimeAwareSliderConfiguration}
        </Configuration>
    </TimeAwareSlider>
    <Toolbar>
      <Views>
        <View Id="ToolbarView" Region="ToolbarRegion" Visible="false" AssociatedHelpTopicKey="Toolbar" />
      </Views>
      <Configuration>
        <Toolbar StickyTools="true">
          {Insert__Toolbar-Tabs}
        <Tab DisplayName="Getting Around"><Group DisplayName="Navigation"><Tool Name="Pan" DisplayName="Pan" DrawMode="None" ImageUri="/Resources/Images/Icons/Toolbar/pan-32.png" ToolTip="Pan" HelpText="Drag the map around to pan." /><Tool Name="ZoomIn" DisplayName="Zoom In" DrawMode="Rectangle" Command="ZoomInToExtent" ImageUri="/Resources/Images/Icons/Toolbar/zoom-in-32.png" ToolTip="Zoom in" HelpText="Drag a rectangle or click the map to zoom in. A large rectangle zooms less. A small rectangle zooms more. To reverse zoom, hold CTRL." /><Tool Name="ZoomOut" DisplayName="Zoom Out" DrawMode="Rectangle" Command="ZoomOutToExtent" ImageUri="/Resources/Images/Icons/Toolbar/zoom-out-32.png" ToolTip="Zoom out" HelpText="Drag a rectangle or click the map to zoom out. A large rectangle zooms less. A small rectangle zooms more. To reverse zoom, hold CTRL." /><Button Name="Initial Extent" Command="ZoomToInitialExtent" ImageUri="/Resources/Images/Icons/Toolbar/zoom-initial-32.png" ToolTip="Zoom to initial extent" /><Button Name="Full Extent" Command="ZoomToFullExtent" ImageUri="/Resources/Images/Icons/Toolbar/zoom-full-32.png" ToolTip="Zoom to full extent" /><Button Name="Previous Extent" Command="ZoomToPreviousExtent" ImageUri="/Resources/Images/Icons/Toolbar/zoom-previous-32.png" ToolTip="Zoom to previous extent" /><Button Name="Next Extent" Command="ZoomToNextExtent" ImageUri="/Resources/Images/Icons/Toolbar/zoom-next-32.png" ToolTip="Zoom to next extent" /></Group><Group DisplayName="Location Info"><Tool Name="PointIdentify" DisplayName="Point Identify" ToolTip="Identify (Point)" DrawMode="Point" Command="Identify" ImageUri="/Resources/Images/Icons/Toolbar/identify-point-32.png" HelpText="Click on the map to identify features at that point. Select from the Layers drop-down list to change which layers display." /></Group><Group DisplayName="Information &amp; Actions"><Region RegionName="AdvancedControlsRegion" /></Group><Group DisplayName="Help">
      {Insert__Toolbar-GettingAround}
    <Button Name="LaunchDefaultHelp" DisplayName="Help" Command="ShowHelpTopic" CommandParameter="" ImageUri="/Resources/Images/Icons/Toolbar/help-32.png" ToolTip="Show help topics" /><Button Name="ClickForHelp" DisplayName="What's This?" Command="InvokeClickHelp" ImageUri="/Resources/Images/Icons/Toolbar/help-cursor-32.png" ToolTip="Enable click help" /></Group></Tab><Tab DisplayName="Maps &amp; Data Sources">
            {Insert__Toolbar-MapsAndDataSources-Groups}
          <Group DisplayName="Map Layers"><Button Name="Show Layers" Command="SwitchToLayerView" ImageUri="/Resources/Images/Icons/Toolbar/layers-32.png" ToolTip="Show layer list" /></Group><Group DisplayName="Map Tips"><Region RegionName="MapTipsRegion" /></Group><Group DisplayName="Query Data"><Button Name="Simple Query" Command="ActivateView" CommandParameter="SimpleQueryBuilderView" ImageUri="/Resources/Images/Icons/Toolbar/query-builder-32.png" ToolTip="Simple query of layers and attributes." /><Button Name="Advanced Query" Command="ActivateView" CommandParameter="QueryBuilderView" ImageUri="/Resources/Images/Icons/Toolbar/query-builder-advanced-32.png" ToolTip="Advanced query of layers and attributes." /></Group><Group DisplayName="Add Data"><Button Name="Add Map Layer" Command="AddMapLayerInteractive" ImageUri="/Resources/Images/Icons/Toolbar/layers-add-32.png" ToolTip="From an external map source, eg. ArcGIS Online or BC Govt." /><Button DisplayName="Add Shapefile" Command="OpenShapefileDialog" ImageUri="/Resources/Images/Icons/Toolbar/shapefile-add-32.png" ToolTip="NB. M and Z shape types are not supported." Name="AddShapefile" /><Button DisplayName="Add CSV file" Command="OpenCsvDialog" ImageUri="/Resources/Images/FileCSV.png" ToolTip="Open a CSV file and add it to the map" /><Button DisplayName="Batch Geocode" Command="OpenExcelFileDialog" ImageUri="/Resources/Images/Icons/Toolbar/geocode-batch-32.png" ToolTip="Open an Excel file and add it to the map" /></Group><Group DisplayName="External Sources"><Button ImageUri="/Resources/Images/Icons/Toolbar/map-google-32.png" DisplayName="Google Maps™" ToolTip="View the current location in Google Maps / Street View" Command="OpenWebPage" CommandParameter="../WMI/GoogleMaps.html" /><Button ImageUri="/Resources/Images/Icons/Toolbar/map-bing-32.png" DisplayName="Bing™ Maps" ToolTip="View the current location in Bing Maps / Birdseye" Command="OpenWebPage" CommandParameter="../WMI/BingMaps.html" /></Group></Tab><Tab DisplayName="Searches and Tasks">
            {Insert__Toolbar-Tasks-Groups}
          <Group DisplayName="Searches"><Button ImageUri="{ViewerConfigUri}../Resources/Images/ParcelByOwner.png" DisplayName="Property Owner Search" ToolTip="Search for a parcel by the property owner" Name="OwnerSearch" Command="RunWorkflowById" CommandParameter="OwnerSearch" /><Button ImageUri="{ViewerConfigUri}../Resources/Images/SearchAddress.png" DisplayName="Address Search" ToolTip="Search for an address" Name="AddressSearch" Command="RunWorkflowById" CommandParameter="AddressSearch" /><Button ImageUri="{ViewerConfigUri}../Resources/Images/ParcelLegal.png" DisplayName="Legal Parcel Search" ToolTip="Search for a property parcel by its legal description" Name="LegalSearch" Command="RunWorkflowById" CommandParameter="LegalSearch" /><Button ImageUri="{ViewerConfigUri}../Resources/Images/ParcelByID.png" DisplayName="Parcel ID Search" ToolTip="Search for a property parcel by its Parcel ID" Name="ParcelIDSearch" Command="RunWorkflowById" CommandParameter="ParcelIDSearch" /></Group><Group DisplayName="Printing"><Button Name="Print Map" Command="PrintMap" ImageUri="/Resources/Images/Icons/Toolbar/print-32.png" ToolTip="Print a map" /></Group><Group DisplayName="Reports"><Button ImageUri="/Resources/Images/Icons/Toolbar/reports-32.png" Module="Reporting" ToolTip="The internal report is for internal use only." Name="Reports" Command="RunReport" DisplayName="Parcel Reports" /></Group><Group DisplayName="Export Tools"><Button Name="Export Map" Command="ShowExportMapDialog" ImageUri="/Resources/Images/Icons/Toolbar/share-map-32.png" ToolTip="Export a map image" /></Group><Group DisplayName="Results"><Button Name="View Results" Command="SwitchToResultsView" ImageUri="/Resources/Images/Icons/Toolbar/search-results-32.png" ToolTip="View recent search results" /><Button Name="View Selected" Command="SwitchToSelectionView" ImageUri="/Resources/Images/Icons/Toolbar/star-32.png" ToolTip="View selected features" /></Group><Group DisplayName="Drawing Tools"><Region RegionName="DrawingToolsRegion" /></Group></Tab><Tab DisplayName="Analysis"><Group DisplayName="Location Information"><Region RegionName="IdentifyRegion" /></Group><Group DisplayName="Measurement Tools"><Region RegionName="MeasurementToolsRegion" /></Group><Group DisplayName="Coordinate Tools"><Region RegionName="CoordinateToolsRegion" /></Group></Tab></Toolbar>
      </Configuration>
    </Toolbar>
    <Watermark>
      <Views>
        <View Id="WatermarkImageView" Region="MapWatermarkOverlayRegion" Type="Geocortex.EssentialsSilverlightViewer.CoreModules.Watermark.ImageWatermarkView" Visible="{WatermarkImageIsVisible}">
          <Configuration ImageUrl="{WatermarkImageUrl}" ImageMode="{WatermarkImageMode}" Opacity="{WatermarkImageOpacity}" Size="{WatermarkImageSize}" Position="{WatermarkImagePosition}" />
        </View>
      </Views>
    </Watermark>
  </Modules>
  <Resources />
  <Libraries />
</Configuration>