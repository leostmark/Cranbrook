    <Dialog>
    </Dialog>
    <Extract>
      <Views>
        <View Id="ExtractView" Visible="false" Region="ModalWindowRegion" />
      </Views>
    </Extract>
    <FeatureClustering>
    </FeatureClustering>
    <Info>
      <Views>
        <View Id="HomePanelView" Visible="{HomePanelIsVisible}" Region="DataRegion">
          <Configuration CanClose="{HomePanelCanClose}" IconUri="{HomePanelIconUri}" Title="{HomePanelTitle}">
            <Content>
                {HomePanelContent}
              </Content>
          </Configuration>
        </View>
      </Views>
    </Info>
    <IWantToMenu>
      <Views>
        <View Region="TopLeftMapRegion" Type="Geocortex.EssentialsSilverlightViewer.CoreModules.IWantToMenu.MenuFloatingView" AssociatedHelpTopicKey="IWantToMenu" />
      </Views>
      <Configuration>
        <MenuRef MenuId="IWantTo" />
        <Menus>
          <Menu Id="IWantTo" Title="I want to...">
            <Items>
              {Insert__Menus-IWantTo-Items}
            <Item Text="Create a printable map" Command="PrintMap" ImageUri="/Resources/Images/Icons/Toolbar/print-32.png" Description="Create a printable map." /><Item Text="Create a parcel report" Command="RunReport" ImageUri="/Resources/Images/Icons/Toolbar/reports-32.png" Description="Run a report." /><Item Text="Draw on the map" ImageUri="/Resources/Images/Icons/Toolbar/draw-freehand-32.png" Description="Draw freehand on the map surface."><Batch><BatchItem Command="ShowAdvancedTools" /><BatchItem Command="SetCurrentTab" CommandParameter="Tasks" /><BatchItem Command="ActivateTool" CommandParameter="MarkupFreehand" /></Batch></Item><Item Text="Measure distance" ImageUri="/Resources/Images/Icons/Toolbar/measure-distance-32.png" Description="Measure length using a polyline tool."><Batch><BatchItem Command="ShowAdvancedTools" /><BatchItem Command="SetCurrentTab" CommandParameter="Analysis" /><BatchItem Command="ActivateTool" CommandParameter="MeasureDistance" /></Batch></Item><Item Text="Share current map" ImageUri="/Resources/Images/Icons/Toolbar/share-map-24.png" Command="CaptureViewerState" Description="Share your current view of the map with someone." /><Item Text="Return to initial map extent" ImageUri="/Resources/Images/Icons/Toolbar/zoom-initial-32.png" Command="ZoomToInitialExtent" Description="Zoom to the initial extent of the map." /><Item Text="Turn map data on/off" ImageUri="/Resources/Images/Icons/Toolbar/layers-32.png" Description="Open the layer list to modify layer visibilities."><Batch><BatchItem Command="SwitchToLayerView" /><BatchItem Command="OpenDataFrame" /></Batch></Item><Item Text="Show/hide advanced toolbar" Command="ToggleAdvancedTools" ImageUri="/Resources/Images/Icons/Toolbar/tools-24.png" Description="Toggle the Advanced Toolbar." /><Item Text="View Results" Command="SwitchToResultsView" ImageUri="/Resources/Images/Icons/Toolbar/search-results-32.png" Description="View the most recent search results." /><Item Text="View Selected" Command="SwitchToSelectionView" ImageUri="/Resources/Images/Icons/Toolbar/star-32.png" Description="View selected features." /><Item Command="RunWorkflowById" CommandParameter="AddressSearch" Description="Search for a property address" ImageUri="{ViewerConfigUri}../Resources/Images/SearchAddress.png" Text="Search for an address" /><Item Command="RunWorkflowById" CommandParameter="ParcelIDSearch" Description="Search for a property by its Parcel ID" ImageUri="{ViewerConfigUri}../Resources/Images/ParcelByID.png" Text="Search by Parcel ID" /><Item Command="RunWorkflowById" CommandParameter="LegalSearch" Description="Search for a property by its legal description" ImageUri="{ViewerConfigUri}../Resources/Images/ParcelLegal.png" Text="Search by legal description" /><Item Command="RunWorkflowById" CommandParameter="OwnerSearch" Description="Search for a property by its Owner's name" ImageUri="{ViewerConfigUri}../Resources/Images/ParcelByOwner.png" Text="Search by Owner's name" /></Items>
          </Menu>
        </Menus>
      </Configuration>
    </IWantToMenu>
    <ScaleDependentBaseMaps />
    <Symbolization LibraryId="Core">
      <Views>
        <View Id="SymbolizationView" Visible="false" Region="DataRegion" Type="Geocortex.EssentialsSilverlightViewer.CoreModules.Symbolization.SymbolizationEditor" AssociatedHelpTopicKey="Symbolization" />
      </Views>
      <Configuration ShowNormalizationOptions="{SymbolizationShowNormalizationOptions}" ShowNewAttributeQuickLink="{SymbolizationShowNewAttributeQuickLink}" DefaultNumberOfBreaks="{SymbolizationDefaultNumberOfBreaks}" DefaultRampFromColor="{SymbolizationDefaultRampFromColor}" DefaultRampToColor="{SymbolizationDefaultRampToColor}" MakeLayerVisibleOnApply="{SymbolizationMakeLayerVisibleOnApply}" AllowMultiFieldsUniqueValueRenderer="{SymbolizationAllowMultiFieldsUniqueValueRenderer}" UniqueValueMaximumFeaturesInLayer="{SymbolizationUniqueValueMaximumFeaturesInLayer}" PreventUniqueValueWhenMaximumFeaturesReached="{SymbolizationPreventUniqueValueWhenMaximumFeaturesReached}" />
    </Symbolization>
    <TimeAwareSlider>
